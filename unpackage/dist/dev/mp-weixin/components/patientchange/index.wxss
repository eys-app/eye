.system-title {
  position: absolute;
  top: 50px;
  left: 10%;
  font-weight: 800;
  color: #FFFFFF;
}
.system-title .weclome {
  font-size: 18px;
}
.system-title .system-title-text {
  font-size: 26px;
  margin-top: 10px;
}
.selected-patient {
  height: 25px;
  line-height: 25px;
  background-color: rgba(255, 166, 42, 0.6);
  color: gray;
  font-size: 14px;
  position: fixed;
  z-index: 99999;
  top: 100px;
  right: 0;
  -webkit-border-radius: 20px 0 0 20px;
          border-radius: 20px 0 0 20px;
  -webkit-box-shadow: 0 0 3px #FFB400;
          box-shadow: 0 0 3px #FFB400;
  padding-left: 5px;
  font-size: 12px;
}
.selected-patient .pa-text-title {
  font-size: 12px;
  color: gray;
  margin-left: 12px;
  width: 90px;
  color: #FFFFFF;
  text-align: left;
}
.selected-patient .pa-text-name {
  color: #6a85f8;
  font-size: 14px;
  font-weight: 700;
  text-align: center;
  padding: 0 3px;
}
.selected-patient .pa-compose {
  float: right;
  margin-right: 20px;
}
.back-model-view {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.back-model-view .content-model-view {
  width: 80%;
  height: 40%;
  background-color: #FFFFFF;
  -webkit-border-radius: 5px;
          border-radius: 5px;
}
.back-model-view .content-model-view .text-view {
  font-size: 12px;
  color: #FFFFFF;
  padding: 5px;
}
.back-model-view .content-model-view .title {
  text-align: center;
  padding: 10px 0;
  font-size: 14px;
  font-weight: 600;
  border-bottom: 1px solid #eaeef3;
}
.back-model-view .content-model-view .sub-title {
  text-align: left;
  padding: 5px 0 5px 20px;
  font-size: 14px;
  color: gray;
  margin-top: -20px;
}
.back-model-view .content-model-view .patient-name {
  text-align: center;
  padding: 5px 0;
  font-size: 22px;
  color: #6a85f8;
  font-weight: 800;
}
.back-model-view .content-model-view .compose-class {
  margin-left: 20px;
  color: #CCCCCC;
}
.back-model-view .content-model-view .submit-button {
  width: 80%;
  margin-top: 20px;
  height: 30px;
  line-height: 30px;
  font-size: 14px;
  margin-bottom: 20px;
  border: none;
}

