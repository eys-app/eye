.selected-patient {
  width: 100%;
  height: 40px;
  line-height: 40px;
  background-color: #FFFFFF;
  border-bottom: 1px solid #F1F1F1;
  -webkit-box-shadow: 5px 5px 10px #acf2d9;
          box-shadow: 5px 5px 10px #acf2d9;
  color: gray;
  padding-left: 10px;
  font-size: 14px;
}
.selected-patient .pa-text-title {
  float: left;
  font-size: 14px;
  color: gray;
  margin-left: 10px;
  width: 90px;
}
.selected-patient .pa-text-name {
  color: #6a85f8;
  font-size: 18px;
  font-weight: 700;
  float: left;
}
.selected-patient .pa-compose {
  float: right;
  margin-right: 20px;
}
.back-model-view {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.back-model-view .content-model-view {
  width: 80%;
  height: 40%;
  background-color: #FFFFFF;
  -webkit-border-radius: 5px;
          border-radius: 5px;
}
.back-model-view .content-model-view .text-view {
  font-size: 12px;
  color: #FFFFFF;
  padding: 5px;
}
.back-model-view .content-model-view .title {
  text-align: center;
  padding: 10px 0;
  font-size: 14px;
  font-weight: 600;
  border-bottom: 1px solid #eaeef3;
}
.back-model-view .content-model-view .sub-title {
  text-align: left;
  padding: 5px 0 5px 20px;
  font-size: 14px;
  color: gray;
  margin-top: -20px;
}
.back-model-view .content-model-view .patient-name {
  text-align: center;
  padding: 5px 0;
  font-size: 22px;
  color: #6a85f8;
  font-weight: 800;
}
.back-model-view .content-model-view .compose-class {
  margin-left: 20px;
  color: #CCCCCC;
}
.back-model-view .content-model-view .submit-button {
  width: 80%;
  margin-top: 20px;
  height: 30px;
  line-height: 30px;
  font-size: 14px;
  margin-bottom: 20px;
  border: none;
}

