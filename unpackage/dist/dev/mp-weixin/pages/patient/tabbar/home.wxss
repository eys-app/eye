.home-view-back .home-view {
  height: 100%;
  width: 100%;
  position: absolute;
}
.home-view-back .home-view .ids {
  width: 100%;
}
.home-view-back .home-view .ids image {
  width: 100%;
  height: 100%;
}
.home-view-back .home-view .content-grid {
  width: 100%;
  height: 100px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  border-bottom: 10px solid #F0F0F0;
}
.home-view-back .home-view .content-grid .cus-grid-item {
  width: calc(100% / 3);
  height: 100%;
  text-align: center;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
.home-view-back .home-view .content-grid .cus-grid-item image {
  width: 40px;
  height: 40px;
}
.home-view-back .home-view .content-grid .cus-grid-item label {
  font-size: 14px;
  color: gray;
  margin-top: 8px;
}
.home-view-back .home-view .class-list {
  width: calc(100% - 20px);
  margin-left: 10px;
  padding-top: 10px;
}
.home-view-back .home-view .class-list .class-list-title .title {
  font-size: 16px;
  font-weight: 700;
  color: black;
  padding-left: 10px;
  padding-bottom: 5px;
}
.home-view-back .home-view .class-list .class-list-title .sub-title {
  font-size: 12px;
  color: gray;
}
.home-view-back .home-view .class-list .class-list-content {
  height: 75px;
  margin-top: 10px;
  border-bottom: 1px solid #F1F1F1;
}
.home-view-back .home-view .class-list .class-list-content image {
  width: 60px;
  height: 60px;
  margin-top: 5px;
  -webkit-border-radius: 5px;
          border-radius: 5px;
  float: left;
}
.home-view-back .home-view .class-list .class-list-content .right-content {
  float: left;
  width: calc(100% - 85px);
  padding-left: 10px;
}
.home-view-back .home-view .class-list .class-list-content .right-content .title {
  font-size: 14px;
  font-weight: 600;
  padding-top: 5px;
  height: 40px;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.home-view-back .home-view .class-list .class-list-content .right-content .sub-title {
  color: gray;
  font-size: 12px;
  height: 33px;
  padding-top: 5px;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.home-view-back .home-view .class-list .class-list-content .right-content .time {
  font-size: 12px;
  color: #b4b4b4;
  height: 18px;
  line-height: 18px;
}

