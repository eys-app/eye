.header-back {
  width: 100%;
  height: 150px;
  background-color: #6a85f8;
  color: #FFFFFF;
}
.header-back .patient-name {
  text-align: center;
  padding-top: 40px;
  font-size: 18px;
  font-weight: 700;
}
.header-back .patient-other {
  text-align: center;
  font-size: 14px;
  font-weight: 600;
  margin-top: 20px;
}
.showHistory {
  width: 60%;
  height: 40px;
  background-color: #FFFFFF;
  -webkit-box-shadow: 3px 3px 3px #dddddd;
          box-shadow: 3px 3px 3px #dddddd;
  margin-left: 20%;
  margin-top: -20px;
  -webkit-border-radius: 20px;
          border-radius: 20px;
  line-height: 40px;
  text-align: center;
  font-size: 14px;
  font-weight: 700;
}
.report-ai {
  padding: 20px;
}
.report-ai .title {
  font-size: 16px;
  font-weight: 700;
}
.report-ai .title .time {
  font-size: 12px;
  color: gray;
  font-weight: 400;
  margin-left: 20px;
}
.report-ai .socre-text {
  margin: 10px 0;
  font-size: 14px;
}
.report-doctor {
  padding: 20px;
}
.report-doctor .report-title {
  font-size: 16px;
  font-weight: 700;
}
.report-doctor .detail-doctor {
  margin: 15px;
}
.report-doctor .detail-doctor image {
  width: 30px;
  height: 30px;
  border: 1px solid #F0AD4E;
  -webkit-border-radius: 50%;
          border-radius: 50%;
  float: left;
}
.report-doctor .detail-doctor .doctor-content {
  float: left;
  height: 35px;
  font-size: 12px;
  margin-left: 10px;
  margin-top: -2px;
}
.report-doctor .detail-doctor .doctor-content .content-top {
  height: 17.5px;
  line-height: 17.5px;
  margin: 0;
  padding-bottom: 2px;
}
.report-doctor .detail-doctor .doctor-content .content-top .top-name {
  padding: 0 5px;
  border-right: 1px solid #F1F1F1;
}
.report-doctor .detail-doctor .doctor-content .content-bottom {
  border-top: 1px solid #F0F0F0;
  height: 17.5px;
  line-height: 17.5px;
  margin: 0;
  padding-top: 2px;
}
.report-doctor .report-doctor-detail {
  margin-top: 20px;
  margin-bottom: 10px;
  -webkit-border-radius: 5px;
          border-radius: 5px;
  border: 1px solid #F0F0F0;
  padding: 10px;
}
.report-doctor .report-doctor-detail .time {
  font-size: 12px;
  color: gray;
  line-height: 20px;
}
.report-doctor .report-doctor-detail .title {
  font-size: 14px;
  line-height: 20px;
  font-weight: 800;
  margin-top: 10px;
}
.report-doctor .report-doctor-detail .content {
  font-size: 14px;
  line-height: 20px;
  margin-top: 5px;
}
.addReportPopup {
  width: 100%;
  background-color: #FFFFFF;
}
.addReportPopup .model-button {
  width: 50%;
  height: 30px;
  line-height: 30px;
  font-size: 12px;
  margin-top: 20px;
  background-color: #09BB07;
  color: #FFFFFF;
}
.addReportPopup .title {
  text-align: center;
  font-size: 14px;
  font-weight: 600;
  line-height: 40px;
  height: 40px;
  border-bottom: 1px solid #F2F2F2;
}
.addReportPopup .sumit-report {
  position: absolute;
  bottom: 10px;
  width: 90%;
  height: 40px;
  background-color: #6A85F8;
  margin-left: 5%;
  line-height: 40px;
  text-align: center;
  color: #FFFFFF;
  font-weight: 700;
  -webkit-border-radius: 5px;
          border-radius: 5px;
}
.addReportPopup .text-title {
  margin: 10px;
  font-size: 12px;
  color: gray;
}
.addReportPopup .text-area {
  border: 1px solid #d1d1d1;
  margin: 10px 20px;
  width: calc(100% - 50px);
  min-height: 100px;
  -webkit-border-radius: 5px;
          border-radius: 5px;
  padding: 5px;
}
.addReportPopup .clear-view {
  width: 100%;
  height: 70px;
  background-color: #FFFFFF;
}
.model-popup {
  width: 80vw;
  max-height: 80vh;
  background-color: #FFFFFF;
  -webkit-border-radius: 5px;
          border-radius: 5px;
  overflow-y: auto;
  padding: 10px;
}
.model-popup-clicked {
  background-color: #F1F1F1;
}
.selected-card {
  margin: 10px 10px 30px 10px;
}
.model-card {
  border-bottom: 1px solid #F2F2F2;
}
.model-card .model-title {
  font-size: 14px;
  font-weight: 700;
  margin-top: 5px;
  margin-bottom: 5px;
}
.model-card .model-content {
  font-size: 14px;
  line-height: 20px;
  margin-bottom: 5px;
}
.doctor-add-report {
  width: 80%;
  height: 30px;
  background-color: #09BB07;
  color: #FFFFFF;
  font-size: 14px;
  font-weight: 600;
  -webkit-border-radius: 20px;
          border-radius: 20px;
  margin-bottom: 30px;
  margin-left: 10%;
  text-align: center;
  line-height: 30px;
}

