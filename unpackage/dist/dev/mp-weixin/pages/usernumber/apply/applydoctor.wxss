.view-page {
  padding: 10px;
  background-color: #F0F0F0;
  height: 100%;
  width: calc(100% - 20px);
  position: absolute;
}
.view-page .card-view {
  margin-top: 10px;
  width: 100%;
  background-color: #FFFFFF;
}
.view-page .card-view .input-fa-view {
  width: 100%;
  height: 40px;
  border-bottom: 1px solid #f4f5f6;
}
.view-page .card-view .input-fa-view .label-left {
  float: left;
  width: 100px;
  height: 100%;
  line-height: 40px;
  margin-left: 10px;
  font-size: 14px;
}
.view-page .card-view .input-fa-view .label-left .tag-red {
  float: left;
  color: red;
}
.view-page .card-view .input-fa-view input {
  float: right;
  height: 40px;
  text-align: right;
  margin-right: 10px;
  font-size: 14px;
  width: calc(100% - 120px);
}
.view-page .card-view .input-fa-view .input-button {
  float: right;
  height: 40px;
  line-height: 40px;
  color: black;
  text-align: right;
  margin-right: 10px;
  font-size: 14px;
  width: calc(100% - 120px);
}
.view-page .card-view .upload-image {
  width: calc(100% - 20px);
  padding: 10px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: start;
  -webkit-justify-content: flex-start;
          justify-content: flex-start;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
}
.view-page .card-view .upload-image .image-item {
  width: calc((100% - 25px) / 3);
  margin-bottom: 10px;
  border: 1px solid #F2F6FC;
  margin-right: 6px;
}
.view-page .card-view .upload-image .image-item .remove-image {
  position: absolute;
  width: 20px;
  height: 20px;
  margin-left: calc((100% - 25px) / 3 - 33px);
  background-color: #FF0000;
  z-index: 9;
}
.view-page .card-view .upload-image .image-item image {
  width: 100%;
  height: 100%;
}
.view-page .card-view .upload-image .add-image {
  border: 1px solid #d9d9d9;
  text-align: center;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: 60%;
  background-position: center;
}
.view-page .submit-button {
  -webkit-border-radius: 20px;
          border-radius: 20px;
  margin-top: 30px;
}

